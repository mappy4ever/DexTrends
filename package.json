{
  "name": "my-bi-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "node scripts/run-tests.js --project=chromium",
    "test:ui": "node scripts/run-tests.js --ui --project=chromium",
    "test:headed": "node scripts/run-tests.js --headed --project=chromium",
    "test:debug": "node scripts/run-tests.js --debug --project=chromium",
    "test:e2e": "node scripts/run-tests.js tests/e2e/ --project=chromium",
    "test:quick": "node scripts/run-tests.js tests/e2e/homepage.spec.ts --project=chromium",
    "test:all-browsers": "node scripts/run-tests.js --project=chromium --project=firefox --project=webkit",
    "test:prod": "npm run build && NODE_ENV=test-prod playwright test --project=chromium",
    "pretest": "npm run typecheck",
    "test:stable": "npm run test:kill-server && npm run test:prod",
    "test:clean": "rm -rf .next && npm run test:kill-server",
    "test:mobile": "node scripts/run-tests.js --project='Mobile Chrome' --project='Mobile Safari'",
    "test:report": "playwright show-report",
    "test:codegen": "playwright codegen",
    "test:iphone": "node scripts/runIPhoneTests.js",
    "test:iphone:watch": "nodemon scripts/runIPhoneTests.js",
    "test:visual": "node scripts/run-tests.js tests/visual/",
    "test:visual:update": "node scripts/run-tests.js tests/visual/ --update-snapshots",
    "test:visual:update-all": "node scripts/update-visual-baselines.js --all",
    "warm-cache": "node scripts/warm-critical-cache.js",
    "warm-cache-all": "node scripts/cache-all-sets.js",
    "cache-progress": "node scripts/monitor-cache-progress.js",
    "verify-cache": "node scripts/verify-cache-warming.js",
    "scrape": "node scripts/runScraper.js",
    "scrape:setup": "node scripts/runScraper.js setup",
    "scrape:gym-leaders": "node scripts/runScraper.js gym-leaders",
    "scrape:elite-four": "node scripts/runScraper.js elite-four",
    "scrape:badges": "node scripts/runScraper.js badges",
    "scrape:games": "node scripts/runScraper.js games",
    "scrape:energy": "node scripts/runScraper.js energy",
    "scrape:maps": "node scripts/runScraper.js maps",
    "scrape:all": "node scripts/runScraper.js all",
    "test:kill-server": "pkill -f 'next dev' || pkill -f 'next start' || true",
    "test:server-check": "curl -f http://localhost:${PORT:-3000}/api/health || echo 'Server not ready'",
    "test:dev-server": "NODE_ENV=test npm run dev",
    "test:full-reset": "npm run test:clean && npm install && npm run test:stable",
    "db:sync-full": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-tcg-database.ts --full",
    "db:sync-delta": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-tcg-database.ts --delta",
    "db:sync-set": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-tcg-database.ts --set",
    "db:sync-pokemon": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-pokemon-database.ts --all",
    "db:sync-pokemon-delta": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-pokemon-database.ts --all --delta",
    "db:sync-pokemon-only": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-pokemon-database.ts --pokemon",
    "db:sync-moves": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-pokemon-database.ts --moves",
    "db:sync-abilities": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-pokemon-database.ts --abilities",
    "db:sync-static": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-pokemon-database.ts --types --natures --berries",
    "db:sync-showdown": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/showdown-sync/sync-showdown-data.ts",
    "db:sync-showdown-tiers": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/showdown-sync/sync-showdown-data.ts --tiers-only",
    "db:sync-prices": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sync-price-history.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
